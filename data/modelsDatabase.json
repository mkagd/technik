  "ocr_patterns": [
    {
      "pattern": "Model[:\\s]*([A-Z0-9\\-\\/]+)",
      "description": "Standardowy format Model: XXX"
    },
    {
      "pattern": "Typ[:\\s]*([A-Z0-9\\-\\/]+)",
      "description": "Format Typ: XXX-XXX"
    },
    {
      "pattern": "Art\\.?\\s*Nr\\.?[:\\s]*([A-Z0-9\\.\\-\\/]+)",
      "description": "Numer artykułu"
    },
    {
      "pattern": "E-Nr\\.?[:\\s]*([A-Z0-9\\(\\)\\/\\-]+)",
      "description": "Numer E (Bosch/Siemens)"
    },
    {
      "pattern": "S-Nr\\.?[:\\s]*([A-Z0-9\\(\\)\\/\\-]+)",
      "description": "Numer seryjny (S-Nr)"
    },
    {
      "pattern": "([A-Z]{2,4}[0-9]{4,8}[A-Z]{0,4})",
      "description": "Ogólny wzorzec modelu"
    },
    {
      "pattern": "(WW[0-9]{2}[A-Z][0-9]{4}[A-Z]{2})",
      "description": "Samsung pralka format"
    },
    {
      "pattern": "(WA[A-Z][0-9]{5}[A-Z]{2})",
      "description": "Bosch pralka format"
    },
    {
      "pattern": "(SMS[0-9]{2}[A-Z][0-9]{2}[A-Z])",
      "description": "Bosch zmywarka format"
    },
    {
      "pattern": "(EW[0-9][A-Z][0-9]{3}[A-Z]{2})",
      "description": "Electrolux format"
    },
    {
      "pattern": "([A-Z]{3}[0-9]{5}[A-Z]{2})",
      "description": "Format trzyliterowy + cyfry"
    }
  ]
}